  "scripts": {
    "build": "tsc",
    "deps:folders": "depcruise --include-only '^src' --validate -T archi src | dot -Tsvg > docs/folders.svg",
    "deps:modules": "depcruise --include-only '^src' --validate -T dot src | dot -Tsvg > docs/modules.svg",
    "lint": "concurrently --names \"TypeScript,ESLint\" \"tsc --project tsconfig.dev.json\" \"eslint . --ext .js,.ts --cache --cache-location .eslint/ --color\"",
